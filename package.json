{
  "name": "@ovh-ux/manager-webpack-dev-server",
  "description": "OVH manager webpack development server configuration",
  "keywords": [
    "webpack",
    "webpack-dev-server"
  ],
  "version": "1.0.0",
  "main": "index.js",
  "repository": "ovh-ux/manager-webpack-dev-server.git",
  "author": "OVH SAS",
  "license": "BSD-3-Clause",
  "scripts": {
    "test": "eslint --quiet .",
    "lint": "eslint --quiet --fix ."
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "base64url": "^3.0.0",
    "cookie": "^0.3.1",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "request": "^2.88.0",
    "time-fix-plugin": "^2.0.3",
    "webpack-dev-server": "^3.1.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-angular": "^7.1.2",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0"
  }
}
